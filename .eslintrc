{
  "parser": "babel-eslint",
  "plugins": [ "angular" ],
  "env": {
    "browser": true,
    "node": false,
  },
  "globals": {
    "document": true,
    "console": true
  },
  "rules": {
    "eqeqeq": 2,
    "curly": 2,
    "quotes": [ 2, "single" ],
    "camelcase": 2,
    "key-spacing": 2,
    "new-cap": 2,
    "no-trailing-spaces": 2,
    "no-unreachable": 2,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-extra-strict": 0,
    "space-after-keywords": 2,
    "no-var": 1,

    "angular/ng_document_service": 0,
    "angular/log": 0
  }
}
